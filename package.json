{
  "name": "@nestjs/cqrs",
  "version": "6.0.0",
  "description": "A lightweight CQRS module for Nest framework (node.js)",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prettier": "prettier src/**/*.ts --write && git status",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public"
  },
  "devDependencies": {
    "@nestjs/common": "6.1.1",
    "@types/node": "11.13.6",
    "@types/jest": "24.0.11",
    "husky": "2.1.0",
    "jest": "24.7.1",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "rxjs": "6.4.0",
    "ts-jest": "24.0.2",
    "typescript": "3.4.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "^6.4.0"
  },
  "lint-staged": {
    "*.ts": ["prettier --write", "git add"]
  }
