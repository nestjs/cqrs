  "devDependencies": {
    "@nestjs/common": "6.0.0",
    "@types/node": "^11.9.4",
    "@types/jest": "^23.3.14",
    "husky": "^0.14.3",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.11.1",
    "rxjs": "^6.4.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.1.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "^6.4.0"
  },
  "lint-staged": {
    "*.ts": ["prettier --write", "git add"]
