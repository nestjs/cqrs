{
  "name": "@nestjs/cqrs",
  "version": "6.1.0",
  "description": "A lightweight CQRS module for Nest framework (node.js)",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prettier": "prettier src/**/*.ts --write && git status",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public"
  },
  "devDependencies": {
    "@nestjs/common": "6.10.14",
    "@nestjs/core": "6.10.14",
    "@types/node": "11.15.2",
    "@types/jest": "24.0.25",
    "husky": "4.0.7",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "ts-jest": "24.3.0",
    "typescript": "3.7.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "^6.4.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}